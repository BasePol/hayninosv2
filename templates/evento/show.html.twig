{% extends 'base.html.twig' %}

{% block title %}Evento{% endblock %}

{% block body %}
    <h1>Evento</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ evento.id }}</td>
            </tr>
            <tr>
                <th>FechaInicio</th>
                <td>{{ evento.fechaInicio ? evento.fechaInicio|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FechaFin</th>
                <td>{{ evento.fechaFin ? evento.fechaFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Titulo</th>
                <td>{{ evento.titulo }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ evento.descripcion }}</td>
            </tr>
            <tr>
                <th>TipoPublico</th>
                <td>{{ evento.tipoPublico }}</td>
            </tr>
            <tr>
                <th>Localidad</th>
                <td>{{ evento.localidad }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ evento.direccion }}</td>
            </tr>
            <tr>
                <th>Precio</th>
                <td>{{ evento.precio }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ evento.rating }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_evento_index') }}">back to list</a>

    <a href="{{ path('app_evento_edit', {'id': evento.id}) }}">edit</a>

    {{ include('evento/_delete_form.html.twig') }}
{% endblock %}
